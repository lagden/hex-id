{
  "name": "@tadashi/hex-id",
  "version": "2.0.0",
  "description": "Creates an unique ID based on the system time.",
  "main": "dist/index.umd.js",
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "report": "nyc report --reporter=text-lcov | coveralls",
    "pretest": "xo",
    "test": "ava",
    "prebuild": "npm test && rm -rf dist",
    "build": "rollup -c"
  },
  "keywords": [
    "id",
    "unique",
    "time",
    "ObjectID"
  ],
  "author": "Thiago Lagden",
  "license": "MIT",
  "engines": {
    "node": ">=12.0.0"
  },
  "devDependencies": {
    "ava": "3.7.0",
    "esm": "3.2.25",
    "rollup": "2.6.1",
    "xo": "0.29.1"
  },
  "esm": {
    "cjs": {
      "dedefault": true
    }
  },
  "ava": {
    "require": [
      "esm"
    ]
  },
  "xo": {
    "semicolon": false,
    "rules": {
      "capitalized-comments": 0
    }
  }
}
